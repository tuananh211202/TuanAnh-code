{
  "name": "zisan",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "npm run tailwind && next dev",
    "build": "npm run tailwind && next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "less": "lessc --js src/lib/styles/antdTheme/antd-custom.less src/lib/styles/antdTheme/antd-theme.css",
    "tailwind": "npx tailwindcss -i src/lib/styles/tailwind-config.css -o src/lib/styles/tailwind.css",
    "tailwind:watch": "npx tailwindcss -i src/lib/styles/tailwind-config.css -o src/lib/styles/tailwind.css --watch"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@reduxjs/toolkit": "^1.8.5",
    "antd": "^4.22.7",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "framer-motion": "^6.3.3",
    "moment": "^2.29.4",
    "next": "^12.1.5",
    "next-pwa": "^5.5.2",
    "next-seo": "^5.4.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.8",
    "redux": "^4.2.0",
    "redux-injectors": "^2.0.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.0",
    "@commitlint/cz-commitlint": "^17.0.0",
    "@types/react": "^18.0.9",
    "commitizen": "^4.2.5",
    "commitlint": "^17.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-sznm": "^1.0.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^12.4.1",
    "next-sitemap": "^2.5.20",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4"
  }
}
